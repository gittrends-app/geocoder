{
  "name": "@gittrends-app/geocoder",
  "version": "5.0.5",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gittrends-app/geocoder.git"
  },
  "main": "./packages/core/dist/index.js",
  "exports": "./packages/core/dist/index.js",
  "types": "./packages/core/dist/index.d.ts",
  "bin": {
    "gittrends-geocoder": "./packages/cli/dist/cli.js"
  },
  "type": "module",
  "files": ["packages/core/dist", "packages/cli/dist", "package.json", "README.md"],
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@1.22.22",
  "workspaces": ["packages/core", "packages/cli"],
  "scripts": {
    "prepare": "husky || true",
    "build": "turbo build",
    "lint": "turbo lint",
    "format": "biome format --write .",
    "test": "turbo test",
    "verify": "turbo lint test build",
    "prepack": "turbo build",
    "release": "release-it"
  },
  "dependencies": {
    "@dnlup/fastify-traps": "^3.1.0",
    "@fastify/swagger": "^9.4.0",
    "@fastify/swagger-ui": "^5.2.0",
    "@keyv/compress-brotli": "^2.0.2",
    "cache-manager": "^6.3.2",
    "consola": "^3.2.3",
    "dayjs": "^1.11.13",
    "fastify": "^5.2.1",
    "fastify-type-provider-zod": "^4.0.2",
    "fetch-retry": "^6.0.0",
    "keyv": "^5.2.3",
    "keyv-file": "^5.1.1",
    "node-geocoder": "^4.4.1",
    "p-queue": "^8.0.1",
    "pretty-format": "^29.7.0",
    "quick-lru": "^7.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@release-it/bumper": "^6.0.1",
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^22.10.5",
    "@vitest/coverage-v8": "^3.0.4",
    "auto-changelog": "^2.5.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-flow": "^4.1.0",
    "husky": "^9.1.7",
    "npm-run-all": "^4.1.5",
    "release-it": "^19.0.0",
    "shx": "^0.3.4",
    "tsc-alias": "^1.8.10",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "turbo": "^2.3.3",
    "type-fest": "^4.33.0",
    "typescript": "^5.7.2",
    "vitest": "^3.0.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release-it": {
    "npm": {
      "publish": false
    },
    "git": {
      "commit": true,
      "commitMessage": "chore(release): v${version}",
      "tag": true,
      "tagName": "v${version}",
      "push": true,
      "requireCleanWorkingDir": false
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    },
    "hooks": {
      "before:init": ["turbo lint test"],
      "after:bump": [
        "yarn install",
        "turbo build",
        "git add yarn.lock packages/core/package.json packages/cli/package.json"
      ]
    },
    "plugins": {
      "@release-it/bumper": {
        "out": ["package.json", "packages/core/package.json", "packages/cli/package.json"]
      }
    }
  }
}
