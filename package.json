{
  "name": "@gittrends-app/geocoder",
  "version": "5.0.8",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gittrends-app/geocoder.git"
  },
  "main": "./packages/core/dist/index.js",
  "exports": "./packages/core/dist/index.js",
  "types": "./packages/core/dist/index.d.ts",
  "bin": {
    "gittrends-geocoder": "./packages/cli/dist/cli.js"
  },
  "type": "module",
  "files": [
    "packages/core/dist",
    "packages/cli/dist",
    "package.json",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "packages/core",
    "packages/cli"
  ],
  "scripts": {
    "prepare": "husky || true",
    "build": "turbo build",
    "lint": "turbo lint",
    "format": "biome format --write .",
    "test": "turbo test",
    "verify": "turbo lint test build",
    "prepack": "turbo build",
    "release": "release-it"
  },
  "dependencies": {
    "@dnlup/fastify-traps": "^3.1.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@keyv/compress-brotli": "^2.0.5",
    "cache-manager": "^7.2.7",
    "consola": "^3.4.2",
    "dayjs": "^1.11.13",
    "fastify": "^5.2.1",
    "fastify-type-provider-zod": "^6.1.0",
    "fetch-retry": "^6.0.0",
    "keyv": "^5.5.5",
    "keyv-file": "^5.3.3",
    "node-geocoder": "^4.4.1",
    "p-queue": "^9.0.1",
    "pretty-format": "^30.2.0",
    "quick-lru": "^7.3.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@release-it/bumper": "^7.0.5",
    "@tsconfig/node18": "^18.2.6",
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "auto-changelog": "^2.5.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-flow": "^4.1.0",
    "husky": "^9.1.7",
    "npm-run-all": "^4.1.5",
    "release-it": "^19.0.0",
    "shx": "^0.4.0",
    "tsc-alias": "^1.8.16",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "turbo": "^2.7.2",
    "type-fest": "^5.3.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release-it": {
    "npm": {
      "publish": false
    },
    "git": {
      "commit": true,
      "commitMessage": "chore(release): v${version}",
      "tag": true,
      "tagName": "v${version}",
      "push": true,
      "requireCleanWorkingDir": false
    },
    "github": {
      "web": true,
      "autoGenerate": true,
      "release": true,
      "releaseName": "v${version}",
      "releaseNotes": "auto-changelog -p"
    },
    "hooks": {
      "before:init": [
        "turbo lint test"
      ],
      "after:bump": [
        "yarn install",
        "turbo build",
        "git add yarn.lock packages/core/package.json packages/cli/package.json"
      ]
    },
    "plugins": {
      "@release-it/bumper": {
        "out": [
          "packages/core/package.json",
          "packages/cli/package.json"
        ]
      }
    }
  }
}
