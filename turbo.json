{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "lint": {
      "dependsOn": ["^lint"]
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "@gittrends-app/geocoder-cli#build": {
      "dependsOn": ["@gittrends-app/geocoder-core#build"]
    }
  }
}
