{
  "name": "@gittrends-app/geocoder-cli",
  "version": "4.0.3",
  "main": "./dist/cli.cjs",
  "types": "./dist/cli.d.ts",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": {
    "gittrends-geocoder": "./dist/cli.cjs"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "lint": "eslint src --ext .ts",
    "build": "shx rm -rf dist && tsup src/cli.ts --minify --format cjs --no-splitting --clean",
    "dev": "tsx watch -r \"dotenv-flow/config\" src/cli.ts",
    "start": "node -r \"dotenv-flow/config\" dist/cli.cjs",
    "release": "release-it",
    "prepack": "npx shx cp ../../README.md dist"
  },
  "dependencies": {
    "@dnlup/fastify-traps": "^3.1.0",
    "@fastify/swagger": "^9.4.0",
    "@fastify/swagger-ui": "^5.2.0",
    "@gittrends-app/geocoder": "file:../core",
    "commander": "^13.0.0",
    "consola": "^3.3.3",
    "fastify": "^5.2.0",
    "fastify-type-provider-zod": "^4.0.2",
    "keyv-file": "^5.1.1",
    "zod": "^3.24.1"
  }
}
