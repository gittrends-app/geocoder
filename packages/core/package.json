    "pretty-format": "^29.7.0",
    "quick-lru": "^7.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node-geocoder": "^4.2.6",
    "@vitest/coverage-v8": "^3.0.4",
    "vitest": "^3.0.4"
  },
  "files": ["dist", "package.json", "README.md"]
