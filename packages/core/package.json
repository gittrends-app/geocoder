{
  "name": "@gittrends-app/geocoder-core",
  "version": "5.0.0",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "lint": "eslint src --ext .ts",
    "build:clean": "shx rm -rf dist tsconfig.tsbuildinfo",
    "build:bundle": "tsup src/index.ts --minify --format esm --clean",
    "build:definitions": "tsc --emitDeclarationOnly",
    "build": "run-s build:clean build:bundle build:definitions",
    "test": "vitest run src",
    "release": "release-it"
  },
  "dependencies": {
    "@keyv/compress-brotli": "^2.0.2",
    "cache-manager": "^6.3.2",
    "fetch-retry": "^6.0.0",
    "keyv": "^5.2.3",
    "keyv-file": "^5.1.1",
    "node-fetch": "^3.3.2",
    "node-geocoder": "^4.4.1",
    "p-queue": "^8.0.1",
    "pretty-format": "^29.7.0",
    "quick-lru": "^7.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node-geocoder": "^4.2.6",
    "type-fest": "^4.31.0"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "release-it": {
    "git": {
      "commit": true,
      "commitMessage": "chore(release): core-v${version}",
      "tag": true,
      "tagName": "core-v${version}",
      "push": true
    },
    "hooks": {
      "after:bump": [
        "yarn install",
        "yarn build",
        "git add ../../yarn.lock",
        "yarn shx rm ../../package-lock.json"
      ]
    }
  }
}
